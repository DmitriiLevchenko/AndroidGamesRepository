<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Shop"
    android:orientation="horizontal"
    android:screenOrientation="portrait" >

    <ImageView
        android:id="@+id/crab_2"
        android:layout_width="138dp"
        android:layout_height="83dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="188dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/crab_2" />

    <ImageView
        android:id="@+id/crab_3"
        android:layout_width="138dp"
        android:layout_height="83dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="188dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/crab_2"
        app:srcCompat="@drawable/crab_3" />

    <ImageView
        android:id="@+id/crab_4"
        android:layout_width="138dp"
        android:layout_height="83dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="188dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/crab_3"
        app:srcCompat="@drawable/crab_4" />

    <ImageView
        android:id="@+id/crab_5"
        android:layout_width="138dp"
        android:layout_height="83dp"
        android:layout_marginStart="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/crab_4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.426"
        app:srcCompat="@drawable/crab_5" />
</androidx.constraintlayout.widget.ConstraintLayout>